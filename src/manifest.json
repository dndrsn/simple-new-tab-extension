{
  "name": "Simple new tab page",
  "description": "Override the new tab page with a simple one that uses the bookmarks bar bookmarks",
  "version": "0.1",
  "incognito": "split",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "chrome://favicon/"
  ],
  "background": {
    "scripts": [
      "js/background.js"
    ]
  },
  "icons": {
    "16": "./icons/bookmark-16.png",
    "32": "./icons/bookmark-32.png",
    "36": "./icons/bookmark-36.png",
    "72": "./icons/bookmark-72.png"
  },
  "manifest_version": 2
}
